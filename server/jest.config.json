{
  "coverageDirectory": "./coverage",
  "testMatch": ["<rootDir>/tests/**/*.spec.js"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "coveragePathIgnorePatterns": [
    "<rootDir>/src/migrations/",
    "<rootDir>/src/repositories/",
    "<rootDir>/src/types/"
  ],
  "setupFilesAfterEnv": ["./tests/setup.js"]
}